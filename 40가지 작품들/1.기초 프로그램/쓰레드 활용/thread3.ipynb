   "source": [
    "import threading\n",
    "\n",
    "def sum(name, value):\n",
    "    for i in range(0, value):\n",
    "        print(f\"{name} : {i}\")\n",
    "        \n",
    "t1 = threading.Thread(target=sum, args=(\"thread number1\",10))\n",
    "t2 = threading.Thread(target=sum, args=(\"thread number2\",10))\n",
    "\n",
    "t1.start()\n",
    "t2.start()\n",
    "\n",
    "print(\"Main Thread\")"
   ]
  }
